<% layout('layout/boilerPlate') %>
<body>
    <h2>Create New Listing</h2>
    <form action="/listing" method="POST">
        <input type="text" name="title" id="" placeholder="Enter title"> <br>
        <input type="text" name="image" id="" placeholder="Enter image url"> <br>
        <input type="text" name="description" id="" placeholder="Enter description"> <br>
        <input type="number" name="price" id="" placeholder="Enter price"> <br>
        <input type="text" name="location" id="" placeholder="Enter location"> <br>
        <input type="text" name="country" id="" placeholder="Enter country"> <br>
        <button>Create</button>
    </form>
</body>
